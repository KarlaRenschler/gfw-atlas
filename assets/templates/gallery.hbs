<div class="l-inner">
	<div class="m-filters">
		<ul id="galleryFilters">
      <li data-value="all">All</li>
      {{#each this.filters}}
	      <li data-value="{{this}}">{{this}}</li>	      
      {{/each}}
		</ul>
	</div>

	<ul class="m-grid -margin">
		{{#each this.gallery}}	
			<li class="m-card -gallery">
				<div class="img" style="background-image: url({{this.thumbnail}})"></div>
				<h3 class="-primary">{{this.title}}</h3>
				<p>{{this.organization}}</p>
			</li>
		{{/each}}
	</ul>	
	<div id="gallery-paginator"></div>
	<div class="btn-submit">
		<a href="#" class="-center">			
			<svg><use xlink:href="#icon-addmore"></use></svg>
			<h3>SUBMIT YOUR MAP</h3>
		</a>
	</div>
</div>
